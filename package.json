  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --writer --plugin=prettier-plugin-solidity '**/*.sol",
      "prettier --write",
      "eslint --fix"
    ],
    "*.{css,less,scss}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  }
}
